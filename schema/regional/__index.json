{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "title": "regionaal",
  "required": [
    "last_generated",
    "proto_name",
    "name",
    "code",
    "results_per_region",
    "ggd",
    "nursing_home",
    "sewer",
    "sewer_per_installation",
    "restrictions",
    "difference",
    "deceased_rivm",
    "deceased_cbs",
    "elderly_at_home",
    "disability_care",
    "behavior"
  ],
