{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "title": "regionaal",
  "required": [
    "last_generated",
    "proto_name",
    "name",
    "code",
    "results_per_region",
    "ggd",
    "nursing_home",
    "sewer",
    "sewer_per_installation"
  ],
  "additionalProperties": false,
  "properties": {
    "last_generated": {
      "type": "string"
    },
    "proto_name": {
      "type": "string",
      "pattern": "^VR[0-9]+$"
    },
    "name": {
      "type": "string",
      "const": { "$data": "1/proto_name" }
    },
    "code": {
      "type": "string",
      "const": { "$data": "1/proto_name" }
    },
    "sewer": {
      "$ref": "sewer.json"
    },
    "sewer_per_installation": {
      "$ref": "sewer_per_installation.json"
    },
    "results_per_region": {
      "$ref": "results_per_region.json"
    },
    "ggd": {
      "$ref": "ggd.json"
    },
    "nursing_home": {
      "$ref": "nursing_home.json"
    },
    "restrictions": {
      "$ref": "restrictions.json"
    }
  }
}
