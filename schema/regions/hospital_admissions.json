{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "title": "region_hospital_admissions",
  "additionalProperties": false,
  "required": [
    "date_of_report_unix",
    "vrcode",
    "hospital_admissions",
    "date_of_insertion_unix"
  ],
  "properties": {
    "date_of_report_unix": {
      "type": "integer"
    },
    "vrcode": {
      "type": "string",
      "pattern": "^VR[0-9]+$"
    },
    "hospital_admissions": {
      "type": "number"
    },
    "date_of_insertion_unix": {
      "type": "integer"
    }
  }
}
