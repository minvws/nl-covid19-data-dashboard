{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "title": "escalation_levels",
  "additionalProperties": false,
  "required": [
    "date_of_report_unix",
    "vrcode",
    "escalation_level",
    "date_of_insertion_unix"
  ],
  "properties": {
    "date_of_report_unix": {
      "type": "integer"
    },
    "vrcode": {
      "type": "string",
      "pattern": "^VR[0-9]+$"
    },
    "escalation_level": {
      "type": "number"
    },
    "date_of_insertion_unix": {
      "type": "integer"
    }
  }
}
