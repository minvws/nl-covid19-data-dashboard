{
  "name": "@corona-dashboard/cms",
  "private": true,
  "version": "1.0.0",
  "license": "EUROPEAN UNION PUBLIC LICENCE v. 1.2",
  "type": "module",
  "scripts": {
    "start": "sanity start",
    "build": "yarn workspace @corona-dashboard/common build && yarn workspace @corona-dashboard/icons build && yarn workspace @corona-dashboard/cli generate-data-structures && sanity build",
    "clean": "npx rimraf dist .sanity-asset-cache .lokalize-reference",
    "nuke": "yarn clean && npx rimraf node_modules",
    "dev": "yarn workspace @corona-dashboard/common build && yarn workspace @corona-dashboard/icons build && yarn workspace @corona-dashboard/cli generate-data-structures && sanity dev",
    "deploy": "yarn workspace @corona-dashboard/common build && yarn workspace @corona-dashboard/icons build && yarn workspace @corona-dashboard/cli generate-data-structures && sanity deploy",
    "typecheck": "tsc --pretty --noEmit",
    "lint": "eslint .",
    "lokalize:import": "ts-node src/lokalize/scripts/import.ts",
    "lokalize:add": "ts-node src/lokalize/scripts/add-new-lokalize-document.ts",
    "lokalize:apply-json-edits": "ts-node src/lokalize/scripts/apply-json-edits.ts",
    "lokalize:generate-types": "ts-node src/lokalize/scripts/generate-types.ts",
    "lokalize:sync-after-release": "ts-node src/lokalize/scripts/sync-after-release.ts",
    "lokalize:sync-after-feature": "ts-node src/lokalize/scripts/sync-after-feature.ts",
    "lokalize:sync-prd-to-dev": "ts-node src/lokalize/scripts/sync-prd-to-dev.ts",
    "elements:add": "ts-node src/studio/scripts/add-chart.ts",
    "format": "prettier --write \"src/**/*.{js,ts,tsx}\" \"*.{js,json,md,yml}\""
  },
  "keywords": [
    "sanity"
  ],
  "dependencies": {
    "@sanity/cli": "^3.12.1",
    "@sanity/dashboard": "^3.1.4",
    "@sanity/language-filter": "^3.0.1",
    "@sanity/types": "^3.12.1",
    "@sanity/ui": "^1.3.2",
    "@sanity/vision": "^3.12.1",
    "change-case": "^4.1.2",
    "dotenv-flow": "^3.2.0",
    "flat": "^5.0.2",
    "fs-extra": "^11.1.1",
    "prompts": "^2.4.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.8.0",
    "react-is": "^18.2.0",
    "sanity": "^3.12.1",
    "sanity-plugin-dashboard-widget-document-list": "^1.0.1",
    "sanity-plugin-media": "^2.0.5",
    "styled-components": "^5.3.9",
    "ts-is-present": "^1.2.2"
  },
  "devDependencies": {
    "@sanity/eslint-config-studio": "^2.0.1",
    "@types/dotenv-flow": "^3",
    "@types/flat": "^5.0.2",
    "@types/fs-extra": "^11",
    "@types/prompts": "2.0.14",
    "@types/react": "17.0.43",
    "@types/styled-components": "^5.1.26",
    "eslint": "^8.6.0",
    "fast-csv": "^4.3.6",
    "meow": "^9.0.0",
    "prettier": "^2.8.8",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.3"
  }
}
