{
  "name": "@corona-dashboard/cli",
  "private": true,
  "version": "1.0.0",
  "dependencies": {
    "meow": "^8.0.0",
    "ts-node": "^9.1.1"
  },
  "scripts": {
    "build": "echo 'No build available because this package is not meant for direct consumption'",
    "test": "echo 'No tests available'",
    "test:ci": "echo 'No tests available'",
    "format": "prettier --write src/**/*.{js,ts,tsx} *.{js,json,md,yml}",
    "lint": "eslint .",
    "type-check": "yarn compile",
    "compile": "tsc --pretty --noEmit",
