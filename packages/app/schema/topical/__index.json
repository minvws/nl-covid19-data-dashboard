{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "title": "topical",
  "additionalProperties": false,
  "required": ["title", "dynamicDescription", "themes", "measures"],
  "properties": {
    "title": {
      "$ref": "multilanguage_string.json"
    },
    "dynamicDescription": {
      "$ref": "multilanguage_string.json"
    },
    "themes": {
      "type": "array",
      "minItems": 1,
      "maxItems": 2,
      "items": {
        "$ref": "theme.json"
      }
    },
    "measures": {
      "$ref": "measures.json"
    }
  }
}
