{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "title": "topical",
  "additionalProperties": false,
  "required": [
    "themes",
    "measures"
  ],
  "properties": {
    "themes": {
      "type": "array",
      "minItems": 1,
      "maxItems": 2,
      "items": {
        "$ref": "theme.json"
      }
    },
    "measures": {
      "$ref": "measures.json"
    }
  }
}
