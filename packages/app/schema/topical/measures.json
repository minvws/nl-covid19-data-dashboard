{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "title": "topical_measures",
  "additionalProperties": false,
  "required": ["title", "dynamicSubtitle", "icon", "measureTiles"],
  "properties": {
    "title": {
      "$ref": "multilanguage_string.json"
    },
    "dynamicSubtitle": {
      "$ref": "multilanguage_string.json"
    },
    "icon": {
      "$ref": "icon.json"
    },
    "measureTiles": {
      "type": "array",
      "minItems": 1,
      "items": {
        "$ref": "measure_tile.json"
      }
    }
  }
}
