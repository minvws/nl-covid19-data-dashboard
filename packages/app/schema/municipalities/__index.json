{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "title": "municipalities",
  "additionalProperties": false,
  "required": [
    "last_generated",
    "proto_name",
    "name",
    "code",
    "hospital_admissions",
    "positive_tested_people"
  ],
  "properties": {
    "last_generated": {
      "type": "string"
    },
    "proto_name": {
      "type": "string",
      "enum": ["MUNICIPALITIES"]
    },
    "name": {
      "type": "string",
      "const": { "$data": "1/proto_name" }
    },
    "code": {
      "type": "string",
      "const": { "$data": "1/proto_name" }
    },
    "hospital_admissions": {
      "type": "array",
      "maxItems": 355,
      "items": {
        "$ref": "hospital_admissions.json"
      }
    },
    "positive_tested_people": {
      "type": "array",
      "maxItems": 355,
      "items": {
        "$ref": "positive_tested_people.json"
      }
    }
  }
}
