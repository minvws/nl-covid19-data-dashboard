{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "gm_collection_vaccine_coverage_per_age_group",
  "type": "object",
  "additionalProperties": false,
  "required": [
    "gmcode",
    "age_group_range",
    "age_group_percentage",
    "fully_vaccinated_percentage",
    "birthyear_range",
    "fully_vaccinated_percentage_label",
    "date_of_insertion_unix",
    "date_unix"
  ],
  "properties": {
    "gmcode": {
      "type": "string",
      "pattern": "^GM[0-9]+$"
    },
    "age_group_range": {
      "type": "string",
      "enum": [
        "12+",
        "12-17",
        "18+"
      ]
    },
    "age_group_percentage": {
      "type": [
        "integer",
        "null"
      ]
    },
    "fully_vaccinated_percentage": {
      "type": [
        "integer",
        "null"
      ]
    },
    "partially_vaccinated_percentage": {
      "type": [
        "integer",
        "null"
      ]
    },
    "partially_or_fully_vaccinated_percentage": {
      "type": [
        "integer",
        "null"
      ]
    },
    "birthyear_range": {
      "type": "string",
      "pattern": "^[0-9]*-[0-9]*$"
    },
    "partially_or_fully_vaccinated_percentage_label": {
      "type": [
        "string",
        "null"
      ]
    },
    "date_unix": {
      "type": "integer"
    },
    "date_of_insertion_unix": {
      "type": "integer"
    },
    "date_of_report_unix": {
      "type": "integer"
    }
  }
}
