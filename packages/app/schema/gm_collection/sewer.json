{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "gm_collection_sewer",
  "type": "object",
  "additionalProperties": false,
  "required": [
    "date_start_unix",
    "date_end_unix",
    "gmcode",
    "average",
    "total_installation_count",
    "date_of_insertion_unix",
    "data_is_outdated"
  ],
  "properties": {
    "date_start_unix": {
      "type": "integer"
    },
    "date_end_unix": {
      "type": "integer"
    },
    "gmcode": {
      "type": "string",
      "pattern": "^GM[0-9]+$"
    },
    "average": {
      "type": "integer"
    },
    "total_installation_count": {
      "type": "number"
    },
    "date_of_insertion_unix": {
      "type": "integer"
    },
    "data_is_outdated": {
      "type": "boolean"
    }
  }
}
