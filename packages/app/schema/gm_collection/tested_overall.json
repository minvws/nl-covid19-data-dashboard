{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "title": "municipalities_tested_overall",
  "additionalProperties": false,
  "required": [
    "date_unix",
    "gmcode",
    "infected",
    "infected_per_100k",
    "infected_per_100k_moving_average",
    "date_of_insertion_unix"
  ],
  "properties": {
    "date_unix": {
      "type": "integer"
    },
    "gmcode": {
      "type": "string",
      "pattern": "^GM[0-9]+$"
    },
    "infected_per_100k": {
      "type": "number"
    },
    "infected_per_100k_moving_average": {
      "type": ["number", "null"]
    },
    "infected": {
      "type": "integer"
    },
    "date_of_insertion_unix": {
      "type": "integer"
    }
  }
}
