{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "title": "escalation_levels",
  "additionalProperties": false,
  "required": [
    "vrcode",
    "level",
    "based_on_statistics_to",
    "based_on_statistics_from",
    "next_determined_unix",
    "last_determined_unix",
    "valid_from_unix",
    "date_of_insertion_unix"
  ],
  "properties": {
    "vrcode": {
      "type": "string",
      "pattern": "^VR[0-9]+$"
    },
    "level": {
      "type": "number",
      "minimum": 1,
      "maximum": 4
    },
    "based_on_statistics_to": {
      "type": "integer"
    },
    "based_on_statistics_from": {
      "type": "integer"
    },
    "next_determined_unix": {
      "type": "integer"
    },
    "last_determined_unix": {
      "type": "integer"
    },
    "valid_from_unix": {
      "type": "integer"
    },
    "date_of_insertion_unix": {
      "type": "integer"
    }
  }
}
