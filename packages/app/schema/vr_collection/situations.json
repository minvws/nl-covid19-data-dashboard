{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "vr_collection_situations",
  "additionalProperties": false,
  "required": [
    "date_start_unix",
    "date_end_unix",
    "vrcode",
    "has_sufficient_responses"
  ],
  "properties": {
    "date_start_unix": {
      "type": "integer"
    },
    "date_end_unix": {
      "type": "integer"
    },
    "vrcode": {
      "type": "string",
      "pattern": "^VR[0-9]+$"
    },
    "has_sufficient_responses": {
      "type": "boolean"
    }
  }
}
