          "type": "integer"
        },
        "date_of_report_unix": {
          "type": "integer"
        },
        "birthyear_range": {
          "type": "string",
          "pattern": "^[0-9]{4}-[0-9]{4}$|^-[0-9]{4}$|^[0-9]{4}-$"
        }
      }
    }
  }
