{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "title": "gm",
  "additionalProperties": false,
  "required": [
    "name",
    "proto_name",
    "code",
    "difference",
    "static_values",
    "hospital_nice",
    "sewer_installation_measurement",
    "sewer_per_installation",
    "sewer",
    "vaccine_coverage_per_age_group",
    "last_generated"
  ],
  "properties": {
    "last_generated": {
      "type": "string"
    },
    "proto_name": {
      "$ref": "#/$defs/gm_code"
    },
    "name": {
      "$ref": "#/$defs/gm_code"
    },
    "code": {
      "$ref": "#/$defs/gm_code"
    },
    "static_values": {
      "$ref": "__static_values.json"
    },
    "difference": {
      "$ref": "__difference.json"
    },
    "hospital_nice": {
      "$ref": "hospital_nice.json"
    },
    "sewer": {
      "$ref": "sewer.json"
    },
    "sewer_per_installation": {
      "$ref": "sewer_per_installation.json"
    },
    "sewer_installation_measurement": {
      "$ref": "sewer_installation_measurement.json"
    },
    "vaccine_coverage_per_age_group": {
      "$ref": "vaccine_coverage_per_age_group.json"
    }
  },
  "$defs": {
    "gm_code": {
      "type": "string",
      "pattern": "^GM[0-9]+$"
    }
  }
}
