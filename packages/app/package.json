  "scripts": {
    "analyze": "cross-env ANALYZE=true yarn build",
    "export": "next export",
    "dev": "next",
    "build": "cross-env NEXT_TELEMETRY_DISABLED=1 && next build",
    "build:nl": "cross-env NEXT_PUBLIC_LOCALE=nl && yarn build",
    "build:en": "cross-env NEXT_PUBLIC_LOCALE=en && yarn build",
    "start": "next start -H 0.0.0.0 -p 8080",
    "start-e2e": "next start -H 0.0.0.0 -p 3000",
    "test": "jest --watch",
    "test:ci": "jest --ci",
    "format": "prettier --write \"src/**/*.{js,ts,tsx}\" \"*.{js,json,md}\"",
    "lint": "eslint .",
    "type-check": "yarn compile",
    "compile": "tsc --pretty --noEmit",
    "download": "node ./infra/download.js",
    "clean": "rimraf .next",
    "vercel-install": "yarn install --frozen-lockfile && yarn download && yarn workspace @corona-dashboard/common build && yarn workspace @corona-dashboard/cms lokalize:export --dataset=$SANITY_DATASET && yarn workspace @corona-dashboard/cli validate-json-all && yarn workspace @corona-dashboard/cms sync-assets",
    "vercel-build:nl": "yarn build:nl && next export",
    "vercel-build:en": "yarn build:en && next export"
  },
  "browserslist": [
    "defaults",
    "not ie < 11"
  ]
