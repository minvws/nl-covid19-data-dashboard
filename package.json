    "eslint-plugin-react-hooks": "^4.1.0",
    "husky": "^4.2.5",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.4.2",
    "json-schema-to-typescript": "^9.1.1",
    "lint-staged": "^10.2.11",
    "next-compose-plugins": "^2.2.0",
    "next-transpile-modules": "^4.1.0",
    "node-sass": "^4.14.1",
    "postcss-flexbugs-fixes": "^4.2.1",
    "postcss-preset-env": "^6.7.0",
