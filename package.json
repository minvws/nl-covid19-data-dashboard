    "react-refresh": "^0.8.3",
    "start-server-and-test": "^1.11.4",
    "typescript": "^4.0.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "yarn run type-check"
    }
  },
  "lint-staged": {
    "*.@(ts|tsx)": [
      "yarn lint",
      "yarn format"
    ]
  },
  "scripts": {
    "analyze": "cross-env ANALYZE=true yarn build",
    "export": "next export",
    "dev": "next",
    "build": "cross-env NEXT_TELEMETRY_DISABLED=1 && next build && next export",
    "start": "next start",
    "test": "jest --watch",
    "test:ci": "jest --ci",
    "format": "prettier --write **/*.{js,ts,tsx} *.{js,json,md,yml}",
    "lint": "eslint .",
    "type-check": "tsc --pretty --noEmit",
    "validate-json": "node schema/validator/validate-json.js",
    "validate-single": "node schema/validator/validate-single.js",
    "generate-typescript": "node schema/validator/generate-typescript.js",
    "e2e": "start-test dev 3000 cy:open",
    "e2e:release": "yarn build && start-test 3000 cy:open",
    "cy:open": "cypress open",
    "e2e:ci": "yarn build && start-test 3000 cy:ci",
    "cy:ci": "cypress run --headless --browser chrome",
    "download": "download https://coronadashboard.rijksoverheid.nl/latest-data.zip -e -o public/"
  },
  "browserslist": [
    "defaults",
    "not ie < 11"
  ]
}
